{
  "name": "animated-react",
  "version": "0.1.0",
  "description": "Animations for React components",
  "scripts": {
    "build": "webpack --progress --config ./webpack.config.js",
    "check": "npm-run-all lint format stylelint",
    "dev": "webpack-dev-server --open --config ./webpack.config.js",
    "format": "prettier --check \"src/**/*.{js,jsx}\"",
    "lint": "eslint . --ext \".js,.jsx\"",
    "postinstall": "node scripts/enhanceGitHooks.js",
    "stylelint": "stylelint src/**/*.css --cache"
  },
  "author": "Aleksander Fret",
  "company": "Sandstream Development",
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@babel/preset-react": "7.8.3",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "html-webpack-plugin": "4.0.4",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.3.0",
    "stylelint-config-recommended": "3.0.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": "prettier --write",
    "*.{js,jsx}": [
      "eslint -c .eslintrc --fix",
      "echo test"
    ],
    "*.css": "stylelint --syntax scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  }
}
